{
  "name": "my-page",
  "description": "My page",
  "version": "0.0.1",
  "scripts": {
    "dev": "run-p watch:*",
    "watch:css": "postcss src/assets/global.scss -o static/global.css -w",
    "watch:dev": "sapper dev",
    "build": "run-s build:css build:sapper",
    "build:css": "NODE_ENV=production postcss src/assets/global.scss -o static/global.css",
    "build:sapper": "sapper build --legacy",
    "build:export": "sapper export --legacy",
    "export": "run-s build:css build:export",
    "start": "node __sapper__/build",
    "serve": "serve ___sapper__/export"
  },
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "compression": "^1.7.1",
    "polka": "next",
    "sirv": "^1.0.0",
    "tailwindcss": "^1.9.6",
    "tippy.js": "^6.2.7"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@rollup/plugin-url": "^5.0.0",
    "cssnano": "^4.1.10",
    "fa-svelte": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.4",
    "postcss-cli": "^8.1.0",
    "postcss-import": "^13.0.0",
    "postcss-scss": "^3.0.2",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.28.0",
    "sass": "^1.27.0",
    "sass-loader": "^10.0.4",
    "svelte": "^3.17.3",
    "svelte-preprocess": "^4.5.2",
    "sveltejs-tippy": "^3.0.0"
  }
}
