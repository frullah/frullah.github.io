{
  "name": "my-page",
  "description": "My page",
  "version": "0.0.1",
  "scripts": {
    "dev": "run-p watch:*",
    "watch:css": "postcss src/assets/global.scss -o static/global.css -w",
    "watch:dev": "sapper dev",
    "build": "run-s build:css build:sapper",
    "build:css": "NODE_ENV=production postcss src/assets/global.scss -o static/global.css",
    "build:sapper": "sapper build --legacy",
    "build:export": "sapper export --legacy",
    "preexport": "test \"$CI\" = true && npx pnpm install -r --store=node_modules/.pnpm-store || echo skiping pnpm install",
    "export": "run-s build:css build:export",
    "start": "node __sapper__/build",
    "serve": "serve ___sapper__/export",
    "lint": "eslint src"
  },
  "homepage": "https://frullah.github.io",
  "dependencies": {
    "compression": "^1.7.4",
    "polka": "^0.5.2",
    "sirv": "^1.0.7",
    "tailwindcss": "^2.0.1",
    "tippy.js": "^6.2.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/runtime": "^7.12.5",
    "@beyonk/svelte-scrollspy": "^2.1.2",
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@jackfranklin/rollup-plugin-markdown": "^0.2.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-url": "^5.0.1",
    "@skeleton-elements/svelte": "^2.0.0",
    "autoprefixer": "^10.0.2",
    "cssnano": "^4.1.10",
    "eslint": "^7.13.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.7",
    "postcss-cli": "^8.3.0",
    "postcss-import": "^13.0.0",
    "postcss-scss": "^3.0.4",
    "rollup": "^2.33.3",
    "rollup-plugin-glob": "^1.0.2",
    "rollup-plugin-svelte": "^6.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.28.10",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "svelte": "^3.29.7",
    "svelte-preprocess": "^4.6.0",
    "svelte-scrollto": "^0.2.0",
    "sveltejs-tippy": "^3.0.0"
  }
}
